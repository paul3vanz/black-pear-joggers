{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "magic-mile": {
      "root": "apps/magic-mile/",
      "sourceRoot": "apps/magic-mile/src",
      "projectType": "application",
      "prefix": "bpj",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/magic-mile",
            "index": "apps/magic-mile/src/index.html",
            "main": "apps/magic-mile/src/main.ts",
            "polyfills": "apps/magic-mile/src/polyfills.ts",
            "tsConfig": "apps/magic-mile/tsconfig.app.json",
            "assets": ["apps/magic-mile/src/favicon.ico", "apps/magic-mile/src/assets"],
            "styles": ["scss/main.scss", "apps/magic-mile/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/magic-mile/src/environments/environment.ts",
                  "with": "apps/magic-mile/src/environments/environment.prod.ts"
                }
              ],
              "baseHref": "/apps/magic-mile/",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "magic-mile:build",
            "port": 4201
          },
          "configurations": {
            "production": {
              "browserTarget": "magic-mile:build:production"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/magic-mile/src/test.ts",
            "polyfills": "apps/magic-mile/src/polyfills.ts",
            "tsConfig": "apps/magic-mile/tsconfig.spec.json",
            "karmaConfig": "apps/magic-mile/karma.conf.js",
            "styles": ["scss/main.scss", "apps/magic-mile/src/styles.scss"],
            "scripts": [],
            "assets": ["apps/magic-mile/src/favicon.ico", "apps/magic-mile/src/assets"]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/magic-mile/tsconfig.app.json", "apps/magic-mile/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
