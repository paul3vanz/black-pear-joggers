{
  "version": 2,
  "projects": {
    "auth": {
      "root": "apps/auth",
      "sourceRoot": "apps/auth",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/auth",
            "outputPath": "dist/apps/auth"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "auth:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "auth:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "auth:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/auth"],
          "options": {
            "jestConfig": "apps/auth/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/auth/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "auth-e2e": {
      "root": "apps/auth-e2e",
      "sourceRoot": "apps/auth-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/auth-e2e/cypress.json",
            "devServerTarget": "auth:serve",
            "tsConfig": "apps/auth-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "auth:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/auth-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "auth"
}
