{
  "npmScope": "black-pear-joggers",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "auth": {
      "tags": []
    },
    "auth-e2e": {
      "tags": [],
      "implicitDependencies": ["auth"]
    },
    "container": {
      "tags": []
    },
    "favicon": {
      "tags": []
    },
    "footer": {
      "tags": []
    },
    "header": {
      "tags": []
    },
    "helpers": {
      "tags": []
    },
    "lazy-load-image": {
      "tags": []
    },
    "stack": {
      "tags": []
    }
  }
}
